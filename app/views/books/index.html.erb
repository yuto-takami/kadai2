<!DOCTYPE html>
<html>
  <head>
  </head>

  <body>
    <h1>Books</h1>
    <% @books.each do |list| %>
      <p>Title</p>
      <span><%= list.title %></span>
      <p>Body</p>
      <span><%= list.body %></span>
    <% end %>



    <h1>New Books</h1>
    <%= form_with model: @book, url:'/books', local:true do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %>
    <h4>Body</h4>
    <%= f.text_area :body %>
    <%= f.submit 'Create Books' %>
    <% end %>

  </body>
</html>